    "stylelint-config-standard-scss": "^14.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.22.0",
    "unplugin-icons": "^22.0.0",
    "vite": "^6.0.11",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pages": "^0.32.4",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vitest": "^2.1.8"
  }
}
