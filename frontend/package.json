{
  "name": "neuris-frontend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=22.9.0"
  },
  "engineStrict": true,
  "scripts": {
    "preserve": "vite build",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "clean": "rm -rf dist/",
    "test": "TZ=UTC vitest run",
    "test:unit:watch": "TZ=UTC vitest watch",
    "test:e2e": "playwright test test/e2e",
    "test:e2e:caselaw": "playwright test test/e2e/caselaw",
    "test:e2e:norms": "playwright test test/e2e/norms",
    "test:e2e:shared": "playwright test test/e2e/shared",
    "test:a11y": "playwright test --project=a11y",
    "test:queries": "playwright test --project=queries --reporter=./test/queries/queries_log_reporter.ts",
    "coverage": "TZ=UTC vitest run --coverage",
    "check": "npm run style:fix && npm test && npm run test:e2e -- --project='chromium' && npm run test:a11y",
    "dev": "vite --host",
    "prettier:check": "f() { prettier --check ${@:-.}; }; f",
    "prettier:fix": "f() { prettier --write ${@:-.}; }; f",
    "eslint:check": "f() { eslint --max-warnings=0 ${@:-.}; }; f",
    "eslint:fix": "f() { eslint --fix --max-warnings=0 ${@:-.}; }; f",
    "stylelint:check": "f() { stylelint ${@:-.}; }; f",
    "stylelint:fix": "f() { stylelint --fix ${@:-.}; }; f",
    "style:check": "f() { vue-tsc && npm run prettier:check $@ && npm run eslint:check $@ && npm run stylelint:check $@; }; f",
    "style:fix": "f() { npm run prettier:fix $@ && npm run eslint:fix $@ && npm run stylelint:fix $@; }; f",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow \"$(. ./parseAllowedLicences.sh)\" --csv --out ./frontend-licence-report.csv"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.1.0",
    "@guardian/prosemirror-invisibles": "^3.1.1",
    "@sentry/integrations": "^7.114.0",
    "@sentry/vue": "^8.33.1",
    "@tiptap/extension-blockquote": "^2.8.0",
    "@tiptap/extension-bold": "^2.8.0",
    "@tiptap/extension-bullet-list": "^2.8.0",
    "@tiptap/extension-color": "^2.8.0",
    "@tiptap/extension-document": "^2.8.0",
    "@tiptap/extension-hard-break": "^2.8.0",
    "@tiptap/extension-history": "^2.8.0",
    "@tiptap/extension-image": "^2.8.0",
    "@tiptap/extension-italic": "^2.8.0",
    "@tiptap/extension-list-item": "^2.8.0",
    "@tiptap/extension-ordered-list": "^2.8.0",
    "@tiptap/extension-paragraph": "^2.8.0",
    "@tiptap/extension-strike": "^2.8.0",
    "@tiptap/extension-subscript": "^2.8.0",
    "@tiptap/extension-superscript": "^2.8.0",
    "@tiptap/extension-table": "^2.8.0",
    "@tiptap/extension-table-cell": "^2.8.0",
    "@tiptap/extension-table-header": "^2.8.0",
    "@tiptap/extension-table-row": "^2.8.0",
    "@tiptap/extension-text": "^2.8.0",
    "@tiptap/extension-text-align": "^2.8.0",
    "@tiptap/extension-text-style": "^2.8.0",
    "@tiptap/extension-underline": "^2.8.0",
    "@tiptap/vue-3": "^2.8.0",
    "@unhead/vue": "^1.11.7",
    "@unleash/proxy-client-vue": "^0.1.2",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "fast-json-patch": "^3.1.1",
    "immer": "^10.1.1",
    "maska": "^3.0.2",
    "pinia": "^2.2.4",
    "resize-observer-polyfill": "^1.5.1",
    "tippy.js": "^6.3.7",
    "vue": "^3.5.11",
    "vue-router": "4.4.5",
    "vue-tsc": "^2.1.6"
  },
  "devDependencies": {
