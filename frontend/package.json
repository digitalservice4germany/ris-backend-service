{
  "name": "neuris-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preserve": "vite build",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "clean": "rm -rf dist/",
    "test": "TZ=UTC vitest run",
    "test:a11y": "playwright test --config=test/a11y/playwright.config.ts",
    "test:e2e": "playwright test",
    "coverage": "vitest run --coverage",
    "check": "vue-tsc --noEmit && npm run style:check && npm test && npm run test:e2e && npm run test:a11y",
    "dev": "vite",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "lint:check": "eslint --max-warnings=0 --ext .js,.ts,.vue --ignore-path .gitignore . && stylelint ./src/**/*.{vue,scss}",
    "lint:fix": "eslint --max-warnings=0 --ext .js,.ts,.vue --ignore-path .gitignore --fix . && stylelint --fix ./src/**/*.{vue,scss}",
    "style:check": "vue-tsc && npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow \"$(. ./parseAllowedLicences.sh)\" --csv --out ./frontend-licence-report.csv"
  },
  "dependencies": {
    "@tiptap/extension-bold": "^2.0.0-beta.28",
    "@tiptap/extension-bullet-list": "^2.0.0-beta.29",
    "@tiptap/extension-color": "^2.0.0-beta.12",
    "@tiptap/extension-document": "^2.0.0-beta.15",
    "@tiptap/extension-history": "^2.0.0-beta.196",
    "@tiptap/extension-image": "^2.0.0-beta.30",
    "@tiptap/extension-italic": "^2.0.0-beta.28",
    "@tiptap/extension-list-item": "^2.0.0-beta.23",
    "@tiptap/extension-ordered-list": "^2.0.0-beta.30",
    "@tiptap/extension-paragraph": "^2.0.0-beta.23",
    "@tiptap/extension-strike": "^2.0.0-beta.29",
    "@tiptap/extension-subscript": "^2.0.0-beta.13",
    "@tiptap/extension-superscript": "^2.0.0-beta.13",
    "@tiptap/extension-table": "^2.0.0-beta.54",
    "@tiptap/extension-table-cell": "^2.0.0-beta.23",
    "@tiptap/extension-table-header": "^2.0.0-beta.25",
    "@tiptap/extension-table-row": "^2.0.0-beta.22",
    "@tiptap/extension-text": "^2.0.0-beta.15",
    "@tiptap/extension-text-align": "^2.0.0-beta.31",
    "@tiptap/extension-text-style": "^2.0.0-beta.26",
    "@tiptap/extension-underline": "^2.0.0-beta.25",
    "@tiptap/vue-3": "^2.0.0-beta.91",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "install": "^0.13.0",
    "material-design-icons-iconfont": "^6.7.0",
    "material-icons": "^1.12.0",
    "pinia": "^2.0.25",
    "resize-observer-polyfill": "^1.5.1",
    "roboto-fontface": "*",
    "vue": "^3.2.37",
    "vue-router": "^4.1.1",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@namics/stylelint-bem": "^7.0.0",
    "@playwright/test": "^1.27.1",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.2",
    "@testing-library/vue": "^6.6.0",
    "@types/node": "^18.8.1",
    "@types/webfontloader": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitest/coverage-c8": "^0.24.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "autoprefixer": "^10.4.11",
    "axe-playwright": "^1.1.11",
    "axios-mock-adapter": "^1.21.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-playwright": "^0.11.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-vue": "^9.6.0",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "fs": "^0.0.1-security",
    "jest-fail-on-console": "^3.0.2",
    "jsdom": "^20.0.0",
    "license-checker": "^25.0.1",
    "playwright": "^1.27.1",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "postcss-scrollbar": "^0.5.1",
    "prettier": "^2.7.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "stylelint": "^14.16.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "vite": "^2.9.13",
    "vite-plugin-pages": "^0.26.0",
    "vitest": "^0.23.1",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.40.1"
  }
}
