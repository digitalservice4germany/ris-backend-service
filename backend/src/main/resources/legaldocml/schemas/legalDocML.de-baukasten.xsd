      <xs:attribute name="name" use="required" type="nameLiterals.inline"/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="required" type="refersToLiterals.inline"/>
   </xs:complexType>
   <xs:complexType name="betroffenesDokument" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "betroffenesDokument" wird genutzt, um das von einer Änderung betroffene Dokument anzugeben.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="href" use="required" type="hrefLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
