      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="fussnote" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "fussnote" wird benutzt, um eine Fußnote in einem beliebigen Inhaltselement einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="HTMLblock" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="marker" use="required" type="markerLiterals"/>
      <xs:attribute name="placement" use="required" type="placementLiterals.fussnote"/>
      <xs:attribute name="placementBase" use="required" type="placementBaseLiterals"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.fussnote"/>
   </xs:complexType>
   <xs:group name="HTMLinline">
      <xs:choice>
         <xs:element name="span" type="span"/>
         <xs:element name="b" type="b"/>
         <xs:element name="i" type="i"/>
         <xs:element name="a" type="a"/>
         <xs:element name="u" type="u"/>
         <xs:element name="sub" type="sub"/>
         <xs:element name="sup" type="sup"/>
         <xs:element name="abbr" type="abbr"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="span" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "span" ist sinngemäß aus HTML übernommen und wird für generische Inlineelemente genutzt.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
      <xs:attribute name="status" use="optional" type="styleLiterals"/>
   </xs:complexType>
   <xs:complexType name="b" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "b" ist sinngemäß aus HTML übernommen und markiert Text, der fett (bold) dargestellt werden soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="i" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "i" ist sinngemäß aus HTML übernommen und markiert Text, der kursiv (italic) dargestellt werden soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="a" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "a" ist sinngemäß aus HTML übernommen und markiert Text, der als Link fungieren soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="href" use="required" type="hrefLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="u" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "u" ist sinngemäß aus HTML übernommen und markiert Text, der unterstrichen (underline) dargestellt werden soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="sub" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "sub" ist sinngemäß aus HTML übernommen und markiert Text, der tiefgestellt (subscript) dargestellt werden soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="sup" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "sup" ist sinngemäß aus HTML übernommen und markiert Text, der hochgestellt (superscript) dargestellt werden soll. Innerhalb dieser Klasse können Fußnoten eingefügt werden.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="abbr" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "abbr" ist sinngemäß aus HTML übernommen und markiert eine Abkürzung als solche. Das ausgeschriebene Wort kann als Attribut angegeben werden.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="ort" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "ort" wird verwendet, um eine Ortsangabe einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="required" type="refersToLiterals"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.ort"/>
   </xs:complexType>
   <xs:complexType name="funktionsbezeichnung" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "funktionsbezeichnung" wird benutzt, um die Funktion der in einem Anschreiben genannten Person, oder in einer Signatur genannten Person in einem nicht-normativen Dokument zu bezeichnen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.funktionsbezeichnung"/>
   </xs:complexType>
   <xs:complexType name="personenname" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "personenname" wird verwendet, um den Namen einer Person einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="required" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="organisation" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "organisation" wird verwendet, um eine Organisiation einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="required" type="refersToLiterals"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.organisation"/>
   </xs:complexType>
   <xs:group name="kontextelement">
      <xs:choice>
         <xs:element name="date" type="datum"/>
         <xs:element name="location" type="ort"/>
         <xs:element name="organization" type="organisation"/>
         <xs:element name="person" type="personenname"/>
         <xs:element name="role" type="funktionsbezeichnung"/>
         <xs:element name="signature" type="signaturInhalt"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="datum" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "datum" wird genutzt, um Datumsangaben zu einem Dokument innerhalb eines Rechtsetzungsdokuments einfügen zu können. Auf Ebene des Schemas werden keine spezifischen Arten von einem Datum unterschieden, da diese strukturell identisch aufgebaut sind. Die Darstellung spezifischer Arten von einem Datum erfolgt ausschließlich durch die Zuordnung von @refersTo-Attributen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="date" use="required" type="dateLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals.datum"/>
   </xs:complexType>
   <xs:complexType name="signaturInhalt" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "signaturInhalt" bündelt mehrere Elemente, die für eine Signatur relevant sind, darunter fallen Ort, Datum, Organisation, Funktionsbezeichnung und Personenname. Die Klasse "signaturInhalt" wird nur bei nicht-normativen Dokumenten verwendet.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="location" type="ort" minOccurs="0"/>
         <xs:element name="date" type="datum" minOccurs="0"/>
         <xs:element name="organization" type="organisation" minOccurs="0"/>
         <xs:element name="role" type="funktionsbezeichnung" minOccurs="0"/>
         <xs:element name="person" type="personenname" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.signaturInhalt"/>
   </xs:complexType>
   <xs:group name="titelelement">
      <xs:choice>
         <xs:element name="docTitle" type="dokumententitel"/>
         <xs:element name="shortTitle" type="kurztitel"/>
         <xs:element name="docStage" type="dokumentenStatus"/>
         <xs:element name="docProponent" type="initiant"/>
         <xs:element name="docType" type="dokumentenTyp"/>
         <xs:element name="docNumber" type="drucksachennummer"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="dokumententitel" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "dokumententitel" wird benutzt, um den Titel eines Dokuments anzugeben. Dies kann beispielsweise der Titel des Rechtsetzungsdokuments insgesamt sein oder der Titel eines Teildokuments des Rechtsetzungsdokuments.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="kurztitel" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "kurztitel" wird benutzt, um den Kurztitel eines Regelungstextes anzugeben.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals.kurztitel"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.kurztitel"/>
   </xs:complexType>
   <xs:complexType name="dokumentenStatus" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "dokumentenStatus" wird benutzt, um den Prozessstatus eines Rechtsetzungsdokuments in der Entwurfsfassung anzugeben (z. B. Referentenentwurf, Regierungsentwurf ect.).</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="initiant" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "initiant" kennzeichnet die Initiantinnen / die Initianten eines Rechtsetzungsdokuments.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="dokumentenTyp" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "dokumentenTyp" wird genutzt, um den Typ des Rechtsetzungsdokuments zu definieren (z. B. Regelungsentwurf eines Gesetzes).</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="drucksachennummer" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "drucksachennummer" wird benutzt, um eine Drucksachennummer in den Dokumentenkopf eines Teildokuments eines Rechtsetzungsdokuments einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals"/>
   </xs:complexType>
   <xs:complexType name="inline" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "inline" bündelt Elemente, die nicht in LegalDocML spezifiziert sind, jedoch für das deutsche Anwendungsprofil LegalDocML. de benötigt werden, um zusätzliche Inhalte eines Dokuments auszuzeichnen. Die Bündelung durch die Klasse "inlineelement" beruht ausschließlich auf modellierungstechnischen Erwägungen (z. B. Amtliche Abkürzung).</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
