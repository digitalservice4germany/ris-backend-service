{
  "name": "neuris-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preserve": "vite build",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "clean": "rm -rf dist/",
    "test": "vitest run",
    "test:a11y": "playwright test --config=test/a11y/playwright.config.ts",
    "test:e2e": "playwright test",
    "coverage": "vitest run --coverage",
    "check": "vue-tsc --noEmit && npm run style:check && npm test && npm run test:e2e && npm run test:a11y",
    "dev": "vite",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint:check -- --fix",
    "style:check": "npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow ''"
  },
  "dependencies": {
    "@tiptap/extension-bold": "^2.0.0-beta.28",
    "@tiptap/extension-bullet-list": "^2.0.0-beta.29",
    "@tiptap/extension-color": "^2.0.0-beta.12",
    "@tiptap/extension-document": "^2.0.0-beta.15",
    "@tiptap/extension-image": "^2.0.0-beta.30",
    "@tiptap/extension-italic": "^2.0.0-beta.28",
    "@tiptap/extension-list-item": "^2.0.0-beta.23",
    "@tiptap/extension-ordered-list": "^2.0.0-beta.30",
    "@tiptap/extension-paragraph": "^2.0.0-beta.23",
    "@tiptap/extension-strike": "^2.0.0-beta.29",
    "@tiptap/extension-subscript": "^2.0.0-beta.13",
    "@tiptap/extension-superscript": "^2.0.0-beta.13",
    "@tiptap/extension-table": "^2.0.0-beta.54",
    "@tiptap/extension-table-cell": "^2.0.0-beta.23",
    "@tiptap/extension-table-header": "^2.0.0-beta.25",
    "@tiptap/extension-table-row": "^2.0.0-beta.22",
    "@tiptap/extension-text": "^2.0.0-beta.15",
    "@tiptap/extension-text-align": "^2.0.0-beta.31",
    "@tiptap/extension-text-style": "^2.0.0-beta.26",
    "@tiptap/extension-underline": "^2.0.0-beta.25",
    "@tiptap/vue-3": "^2.0.0-beta.91",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "material-design-icons-iconfont": "^6.7.0",
    "roboto-fontface": "*",
    "vue": "^3.2.37",
    "vue-router": "^4.1.1",
    "vuetify": "^3.0.0-beta.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@digitalservice4germany/style-dictionary": "^0.0.4",
    "@playwright-testing-library/test": "^4.3.0",
    "@playwright/test": "^1.24.1",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/user-event": "^14.3.0",
    "@testing-library/vue": "^6.6.0",
    "@types/node": "^18.6.3",
    "@types/webfontloader": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vuetify/vite-plugin": "^1.0.0-alpha.0",
    "autoprefixer": "^10.4.7",
    "axe-playwright": "^1.1.11",
    "babel-loader": "^8.2.5",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "fs": "^0.0.1-security",
    "jsdom": "^20.0.0",
    "license-checker": "^25.0.1",
    "playwright": "^1.24.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.0",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vite-plugin-pages": "^0.25.0",
    "vitest": "^0.19.1",
    "vue-cli-plugin-vuetify": "^2.5.1",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.39.0",
    "vuetify-loader": "^2.0.0-alpha.0"
  }
}
