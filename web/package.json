{
  "name": "typescript-vite-application-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vite preview",
    "build": "vue-tsc && vite build",
    "test:e2e": "playwright test",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow ''",
    "check": "vue-tsc --noEmit && npm run style:check && npm test && npm run test:e2e && npm run test:a11y",
    "clean": "rm -rf dist/",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint:check -- --fix",
    "preserve": "vite build",
    "storybook": "start-storybook -p 6006 -h localhost",
    "storybook:build": "build-storybook",
    "storybook:deploy": "storybook-to-ghpages --source-branch=main",
    "style:check": "npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "test": "vitest run",
    "test:a11y": "playwright test --config=test/a11y/playwright.config.ts"
  },
  "dependencies": {
    "vue": "^3.2.33",
    "vuestic-ui": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@digitalservice4germany/style-dictionary": "^0.0.4",
    "@playwright-testing-library/test": "^4.2.0",
    "@playwright/test": "^1.22.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/storybook-deployer": "^2.8.11",
    "@storybook/vue3": "^6.4.22",
    "@testing-library/dom": "^8.13.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "autoprefixer": "^10.4.7",
    "axe-playwright": "^1.1.11",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.2",
    "eslint": "^8.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.11",
    "eslint-plugin-vue": "^8.7.1",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "jsdom": "^19.0.0",
    "license-checker": "^25.0.1",
    "playwright": "^1.22.0",
    "postcss": "^8.4.13",
    "prettier": "2.6.2",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vitest": "^0.12.4",
    "vue-cli-plugin-vuestic-ui": "~1.0.5",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.34.13"
  }
}
